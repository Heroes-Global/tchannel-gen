  "dependencies": {
    "git-repo-info": "1.1.2",
    "logtron": "8.3.4",
    "minimist": "1.1.1",
    "my-local-ip": "1.0.0",
    "process-reporter": "1.0.0",
    "ringpop": "git+ssh://git@github.com:uber/ringpop#tchannel-beta6",
    "tchannel": "2.0.0",
    "uber-statsd-client": "1.3.2",
    "uncaught-exception": "6.0.1",
